# Firebase 認證提供商設定
# 設為 'true' 啟用對應的登入方式

# Google 登入 (預設啟用)
VITE_ENABLE_GOOGLE_AUTH=true

# Facebook 登入
VITE_ENABLE_FACEBOOK_AUTH=false

# GitHub 登入
VITE_ENABLE_GITHUB_AUTH=false

# Twitter 登入
VITE_ENABLE_TWITTER_AUTH=false

# Microsoft 登入
VITE_ENABLE_MICROSOFT_AUTH=false

# Apple 登入
VITE_ENABLE_APPLE_AUTH=false

# Yahoo 登入
VITE_ENABLE_YAHOO_AUTH=false

# Email/Password 登入
VITE_ENABLE_EMAIL_AUTH=false

# 手機號碼登入
VITE_ENABLE_PHONE_AUTH=false